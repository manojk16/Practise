#include <iostream>
using namespace std;

class Test{
	int id;
	string name;
public:
	Test():id(0),name(""){}
	Test(int id, string name):id(id), name(name){}
	void print(){
		cout<<"id-->"<<id<<"Name-->"<<name<<endl;
	}
};

int main() {
	cout << "Welocme to Overloading" << endl; // prints Welocme to Overloading
	Test test1(10,"Mike");
	return 0;
}

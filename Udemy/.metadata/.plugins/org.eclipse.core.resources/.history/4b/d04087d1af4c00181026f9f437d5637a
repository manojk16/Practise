#include <iostream>
#include <list>
using namespace std;

int main() {
	cout << "Welcome to list!!!" << endl; // prints Welcome to list!!!

	list<int> numbers;


	numbers.push_back(1);
	numbers.push_back(2);
	numbers.push_back(3);

	list<int>::iterator iter;


	for(iter=numbers.begin();iter!=numbers.end();iter++){
		cout<<*iter<<endl;

	}


	return 0;
}

#include <iostream>
#include <map>
using namespace std;
class Person{
	int age;
	string name;
public:
	Person(int years, string n):age(years),name(n){}

	void print(){
		cout << "Name" << name << "Age " << age <<endl;
	}
};
int main() {
	cout << "Custome Object as Map Value" << endl; // prints Custome Object as Map Value

	return 0;
}

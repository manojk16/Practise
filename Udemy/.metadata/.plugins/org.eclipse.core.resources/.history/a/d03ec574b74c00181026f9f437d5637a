#include <iostream>
#include <map>
using namespace std;

int main() {
	cout << "Welcome to Map" << endl; // prints Welcome to Map

	map<string, int> ages;
	ages["Raj"]=40;
	ages["Abhay"]=30;
	ages["Vicky"]=50;
	ages["aanu"]=04;


	cout<<"Age of Aanu:: "<< ages["aanu"]<<endl;




	return 0;
}

/*
	sets only stores the uniques element and inorder
	to finding the unique objects sets are very good


*/


#include <iostream>
#include <set>
using namespace std;

int main() {
	cout << "Welocme to Sets !!" << endl; // prints Welocme to Sets !!

	set<int> numbers;
	numbers.insert(30);
	numbers.insert(10);
	numbers.insert(20);
	numbers.insert(40);


	return 0;
}
